{
  "name": "vue3-admin",
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint src/* --ext .js,.vue",
    "lint:fix": "eslint --fix src/* --ext .js,.vue"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,vue}": [
      "eslint --cache --fix",
      "git add"
    ]
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "keywords": [
    "vue",
    "vue3",
    "vue3-admin"
  ],
  "dependencies": {
    "axios": "^0.21.1",
    "element-plus": "^1.0.2-beta.32",
    "vue": "^3.0.4",
    "vue-router": "^4.0.4",
    "vuex": "^4.0.0-rc.2"
  },
  "devDependencies": {
    "@rollup/plugin-dynamic-import-vars": "^1.1.1",
    "@rollup/plugin-eslint": "^8.0.1",
    "@vitejs/plugin-vue": "^1.1.4",
    "@vue/compiler-sfc": "^3.0.5",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.20.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-vue": "^7.6.0",
    "husky": "^5.1.1",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "lint-staged": "^10.5.4",
    "vite": "^2.0.2",
    "vue-eslint-parser": "^7.5.0",
    "yorkie": "^2.0.0"
  }
}
